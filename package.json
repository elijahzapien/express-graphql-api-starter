{
  "name": "express-graphql-api-starter",
  "version": "0.0.2",
  "engines": {
    "node": ">=7.0.0"
  },
  "private": true,
  "description": "boilerplate for building a GraphQL API using Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "babel src --ignore src/scripts -d dist",
    "debug": "DEBUG='Server,Server:*'",
    "setup": "npm run debug node scripts/setup.js",
    "dev": "npm run debug nodemon server.js",
    "production": "npm run build && node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elijahzapien/express-graphql-api-starter.git"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "debug": "3.0.1",
    "nodemon": "1.12.0"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "dotenv": "4.0.0",
    "express": "4.15.4",
    "express-graphql": "0.6.11",
    "graphql": "0.11.2",
    "jsonwebtoken": "7.4.3",
    "morgan": "1.8.2"
  }
}
