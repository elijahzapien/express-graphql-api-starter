{
  "name": "express-graphql-api-starter",
  "version": "0.0.1",
  "engines": {
    "node": ">=7.0.0"
  },
  "private": true,
  "description": "boilerplate for building a GraphQL API using Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "babel src --ignore src/scripts -d dist",
    "debug": "DEBUG='Server,Server:*'",
    "setup": "npm run debug node scripts/setup.js",
    "dev": "npm run debug nodemon server.js",
    "production": "npm run build && node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elijahzapien/express-graphql-api-starter.git"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-module-resolver": "2.4.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "debug": "2.3.3",
    "nodemon": "1.11.0"
  },
  "dependencies": {
    "bcrypt": "0.8.7",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "express-graphql": "0.6.1",
    "graphql": "0.8.2",
    "jsonwebtoken": "7.1.9",
    "morgan": "1.7.0"
  }
}
